<html lang="en-US">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <link rel="icon" type="image/png" href="https://i.ibb.co/VLcBLNX/favicon.png">
    <title>{{htmlWebpackPlugin.options.title}}</title>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-141502377-1"></script>
    <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', 'UA-141502377-1');
    </script>
    <style>
        .usermessage-container {
            background: #EBEBEB;
            float: left;
            width: 100%;
            text-align: center;
        }

        p.usermessage {
            font-size: 18px;
            line-height: 1.6em;
            color: #282828;
            padding: 0;
            font-weight: 100;
            -webkit-font-smoothing: antialiased;
            font-size: 24px;
        }
    </style>
</head>

<body>
    <div class="usermessage-container" id="mobile-compatible" style="display:none">
        <p class="usermessage" id="mobile-compatible-message">Sorry, this website has not been optimised for this screen size. For the best experience, please view on a laptop or desktop device.</p>
    </div>
    <div class="main-content--container" id="fullpage" style="display:none">
        <div class="title-container section fp-auto-height">
            <h1 class="title-container--title"></h1>
        </div>
        {{>./components/section-two/section-two}}
        {{>./components/scatter-chart/scatter-chart}}
        <div id="stopHere"></div>
        {{>./components/predict-grades/predict-grades}}
        {{>./components/donut-chart/donut-chart}}
        {{>./components/horizontal-chart/horizontal-chart}}
        <div id="stopHereHoriz"></div>
        {{>./components/waffle-chart/waffle-chart}}
        {{>./components/section-eight/section-eight}}
        {{>./components/section-nine/section-nine}}
    </div>
    <script>
        document.querySelector('.main-content--container').style.display = 'block';
    </script>
</body>

</html>
